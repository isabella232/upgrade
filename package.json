{
  "name": "prisma-upgrade",
  "version": "0.0.1",
  "description": "Upgrade is a CLI tool to help Prisma 1 users upgrade to Prisma 2.",
  "main": "dist/api/index.js",
  "files": [
    "bin",
    "dist"
  ],
  "bin": {
    "prisma-upgrade": "bin/upgrade.js"
  },
  "scripts": {
    "test": "mocha dist/api/index_test.js --timeout 10s",
    "test:watch": "mocha -w dist/api/index_test.js",
    "generate": "pegjs src/prisma2/parser/index.pegjs && ./scripts/cjs2es.js",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "check": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prisma/upgrade.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/prisma/upgrade/issues"
  },
  "homepage": "https://github.com/prisma/upgrade#readme",
  "dependencies": {
    "@prisma/sdk": "2.0.0-beta.3",
    "@types/graphlib": "2.1.5",
    "arg": "4.1.3",
    "graphlib": "2.1.8",
    "graphql": "15.0.0",
    "internal": "2.0.27",
    "kleur": "3.0.3",
    "module": "1.2.5",
    "prisma1": "1.34.11",
    "prismafile": "1.0.1",
    "prompts": "2.3.2",
    "redent": "3.0.0",
    "resolve": "1.15.1"
  },
  "devDependencies": {
    "@prisma/cli": "2.0.0-beta.3",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-node-resolve": "7.1.1",
    "@rollup/plugin-sucrase": "3.0.0",
    "@types/execa": "2.0.0",
    "@types/mocha": "7.0.2",
    "@types/mysql2": "github:types/mysql2",
    "@types/prompts": "2.0.5",
    "execa": "4.0.0",
    "mariadb": "2.3.1",
    "mocha": "7.1.1",
    "mysql2": "2.1.0",
    "pegjs": "0.10.0",
    "rollup": "2.3.3",
    "testaway": "2.0.1",
    "typescript": "3.8.3"
  }
}
