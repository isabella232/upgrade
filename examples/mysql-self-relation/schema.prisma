datasource db {
  provider = "mysql"
  url      = "mysql://root:prisma@0.0.0.0:3307/mysql-self-relation@dev"
}

model User {
  id         String  @default(cuid()) @id
  invitedBy  String?
  User       User?   @relation("UserToUser_invitedBy", fields: [invitedBy], references: [id])
  other_User User[]  @relation("UserToUser_invitedBy")

  @@index([invitedBy], name: "invitedBy")
}