datasource db {
  provider = "postgresql"
  url      = "postgres://m@localhost:5432/prisma_test?schema=postgres-self-relation%24dev"
}

model User {
  id         String  @default(cuid()) @id
  invitedBy  String? @unique
  User       User?   @relation("UserToUser_invitedBy", fields: [invitedBy], references: [id])
  other_User User?   @relation("UserToUser_invitedBy")
}