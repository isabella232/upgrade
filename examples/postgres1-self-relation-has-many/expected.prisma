datasource db {
  provider = "postgresql"
  url      = "postgres://m@localhost:5432/prisma_test?schema=postgres1-self-relation-has-many%24dev"
}

model User {
  createdAt     DateTime
  id            String   @default(cuid()) @id
  invitedUserId String
  updatedAt     DateTime
  User          User     @relation("UserToUser_invitedUserId", fields: [invitedUserId], references: [id])
  other_User    User[]   @relation("UserToUser_invitedUserId")
}