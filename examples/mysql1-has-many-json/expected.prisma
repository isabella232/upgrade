model Post {
  authorId  String
  createdAt DateTime
  id        String   @default(cuid()) @id
  updatedAt DateTime
  User      User     @relation(fields: [authorId], references: [id])
  @@index([authorId], name: "authorId")
}

model User {
  createdAt DateTime
  id        String   @default(cuid()) @id
  meta      Json?
  updatedAt DateTime
  Post      Post[]
}