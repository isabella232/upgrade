model Media {
  id          String  @default(cuid()) @id
  title       String?
  publisherId String
  User        User    @relation(fields: [publisherId], references: [id])
  @@index([publisherId], name: "publisherId")
}

model User {
  id        String  @default(cuid()) @id
  firstName String
  lastName  String
  Media     Media[]
}