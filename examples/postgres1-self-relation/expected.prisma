datasource db {
  provider = "postgresql"
  url      = "postgres://m@localhost:5432/prisma_test?schema=postgres1-self-relation%24dev"
}

model User {
  createdAt   DateTime
  id          String   @default(cuid()) @id
  invitedById String?
  updatedAt   DateTime
  User        User?    @relation("UserToUser_invitedById", fields: [invitedById], references: [id])
  other_User  User[]   @relation("UserToUser_invitedById")
}