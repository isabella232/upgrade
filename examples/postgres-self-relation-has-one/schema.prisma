datasource db {
  provider = "postgresql"
  url      = "postgres://root:prisma@0.0.0.0:5433/prisma?schema=postgres-self-relation-has-one%24dev"
}

model User {
  bestFriend String?
  id         String  @default(cuid()) @id
  User       User?   @relation("UserToUser_bestFriend", fields: [bestFriend], references: [id])
  other_User User[]  @relation("UserToUser_bestFriend")
}