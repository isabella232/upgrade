datasource db {
  provider = "postgresql"
  url      = "postgres://root:prisma@0.0.0.0:5433/prisma?schema=postgres-self-relation-many-to-many%24dev"
}

model User {
  id          String  @default(cuid()) @id
  invitedUser String?
  User        User?   @relation("UserToUser_invitedUser", fields: [invitedUser], references: [id])
  other_User  User[]  @relation("UserToUser_invitedUser")
}