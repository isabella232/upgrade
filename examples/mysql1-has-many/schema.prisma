datasource db {
  provider = "mysql"
  url      = "mysql://root:prisma@0.0.0.0:3308/mysql1-has-many@dev"
}

model Post {
  createdAt DateTime
  id        String   @default(cuid()) @id
  updatedAt DateTime
  User      User[]   @relation(references: [id])
}

model User {
  createdAt DateTime
  id        String   @default(cuid()) @id
  updatedAt DateTime
  Post      Post[]   @relation(references: [id])
}