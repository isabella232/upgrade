datasource db {
  provider = "mysql"
  url      = "mysql://root@localhost:3306/prisma_test"
}

model Profile {
  createdAt DateTime
  id        String   @default(cuid()) @id
  updatedAt DateTime
  userId    String   @unique
  User      User     @relation(fields: [userId], references: [id])
}

model User {
  createdAt DateTime
  id        String   @default(cuid()) @id
  updatedAt DateTime
  Profile   Profile
}