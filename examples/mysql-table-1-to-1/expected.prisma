datasource db {
  provider = "mysql"
  url      = "mysql://root@localhost:3306/prisma_test"
}

model Settings {
  id   String  @default(cuid()) @id
  user String?
  User User?   @relation(fields: [user], references: [id])
  @@index([user], name: "user")
}

model User {
  id       String     @default(cuid()) @id
  Settings Settings[]
}