datasource db {
  provider = "postgresql"
  url      = "postgres://root:prisma@0.0.0.0:5433/prisma?schema=postgres-has-many%24dev"
}

model Post {
  id   String  @default(cuid()) @id
  user String?
  User User?   @relation(fields: [user], references: [id])
}

model User {
  id   String @default(cuid()) @id
  Post Post[]
}