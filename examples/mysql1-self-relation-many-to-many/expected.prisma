datasource db {
  provider = "mysql"
  url      = "mysql://root@localhost:3306/prisma_test"
}

model User {
  createdAt DateTime
  id        String   @default(cuid()) @id
  updatedAt DateTime
  User_A    User[]   @relation("UserInvitation", references: [id])
  User_B    User[]   @relation("UserInvitation", references: [id])
}