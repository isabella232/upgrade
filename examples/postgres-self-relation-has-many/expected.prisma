datasource db {
  provider = "postgresql"
  url      = "postgres://m@localhost:5432/prisma_test?schema=postgres-self-relation-has-many%24dev"
}

model User {
  id          String  @default(cuid()) @id
  invitedUser String?
  User        User?   @relation("UserToUser_invitedUser", fields: [invitedUser], references: [id])
  other_User  User[]  @relation("UserToUser_invitedUser")
}