model AUser {
  id        String   @default(cuid()) @id
  updatedAt DateTime
  createdAt DateTime
  Post      Post[]
}

model Post {
  id        String   @default(cuid()) @id
  updatedAt DateTime
  createdAt DateTime
  authorId  String
  AUser     AUser    @relation(fields: [authorId], references: [id])
  @@index([authorId], name: "authorId")
}