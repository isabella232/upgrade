model AUser {
  createdAt DateTime
  id        String   @default(cuid()) @id
  updatedAt DateTime
  Post      Post[]
}

model Post {
  authorId  String
  createdAt DateTime
  id        String   @default(cuid()) @id
  updatedAt DateTime
  AUser     AUser    @relation(fields: [authorId], references: [id])
  @@index([authorId], name: "authorId")
}