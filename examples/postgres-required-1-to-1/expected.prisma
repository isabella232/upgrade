datasource db {
  provider = "postgresql"
  url      = "postgres://m@localhost:5432/prisma_test?schema=postgres-required-1-to-1%24dev"
}

model Profile {
  bio  String
  id   String @default(cuid()) @id
  User User
}

model User {
  email   String  @unique
  id      String  @default(cuid()) @id
  profile String? @unique
  Profile Profile @relation(fields: [profile], references: [id])
}