model Media {
  id          String  @default(cuid()) @id
  title       String?
  publisherId String
  User        User    @relation(fields: [publisherId], references: [id])
}

model User {
  id        String  @default(cuid()) @id
  firstName String
  lastName  String
  Media     Media[]
  WebTV     WebTV[]
}

model WebTV {
  id          String @default(cuid()) @id
  publisherId String
  User        User   @relation(fields: [publisherId], references: [id])
}