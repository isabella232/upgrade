datasource db {
  provider = "mysql"
  url      = "mysql://root:prisma@0.0.0.0:3308/mysql1-has-one-optional@dev"
}

model Profile {
  createdAt DateTime
  id        String   @default(cuid()) @id
  updatedAt DateTime
  User      User[]   @relation(references: [id])
}

model User {
  createdAt DateTime
  id        String    @default(cuid()) @id
  updatedAt DateTime
  Profile   Profile[] @relation(references: [id])
}