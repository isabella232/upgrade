datasource db {
  provider = "postgresql"
  url      = "postgres://root:prisma@0.0.0.0:5433/prisma?schema=postgres-table-has-many-required%24dev"
}

model Post {
  id   String @default(cuid()) @id
  User User[] @relation(references: [id])
}

model User {
  id   String @default(cuid()) @id
  Post Post[] @relation(references: [id])
}